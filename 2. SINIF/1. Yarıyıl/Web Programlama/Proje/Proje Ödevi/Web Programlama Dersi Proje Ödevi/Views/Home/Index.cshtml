
@{
    ViewBag.Title = Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.AnaSayfa;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

<body>

    <br /><br />
   
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" src="https://i.hizliresim.com/16k91p.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="https://i.hizliresim.com/MVW55N.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="https://i.hizliresim.com/jgjPJr.jpg" alt="First slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <section class="feature-area section-gap" id="service">
        <div class="container">
            <div class="row d-flex justify-content-center">
                <div class="col-md-8 pb-40 header-text">
                    <h1>@Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.MüşterilerimizeSunduğumuzHizmetler</h1>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4 col-md-6">
                    <div class="single-feature">
                        <h4><span class="lnr lnr-user"></span>@Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.UzmanTeknisyenler</h4>
                        <p>
                            @Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.AlanındaUzman
                        </p>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6">
                    <div class="single-feature">
                        <h4><span class="lnr lnr-license"></span>@Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.ProfesyonelServis</h4>
                        <p>
                            @Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.BizimMüşterilerimizAslaYoldaKalmıyor
                        </p>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6">
                    <div class="single-feature">
                        <h4><span class="lnr lnr-phone"></span>@Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.BüyükDestek</h4>
                        <p>
                            @Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.HerhangiBirSıkıntıİle
                        </p>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6">
                    <div class="single-feature">
                        <h4><span class="lnr lnr-rocket"></span>@Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.TeknikBeceriler</h4>
                        <p>
                          @Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.TeknisyenlerimizİşleriniBüyük
                        </p>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6">
                    <div class="single-feature">
                        <h4><span class="lnr lnr-diamond"></span>@Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.ÖdemeKolaylığı</h4>
                        <p>
                           @Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.BizimİçinPara
                        </p>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6">
                    <div class="single-feature">
                        <h4><span class="lnr lnr-bubble"></span>@Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.OlumluYorumlar</h4>
                        <p>
                           @Web_Programlama_Dersi_Proje_Ödevi.Resources.hometext.SizlerdenAldığımızOlumluYorumlar
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>

</body>
